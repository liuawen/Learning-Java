<%!
		public String getNow(){
			SimpleDateFormat formatter = new SimpleDateFormat("yyyy/MM/dd hh:mm:ss");
			Date currentTime = new Date();
			return formatter.format(currentTime);
		}
%>
<%=getNow() %>
</body>
</html>